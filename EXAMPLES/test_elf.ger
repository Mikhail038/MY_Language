
var sum (var x, var y)
{
    var a = 0;

    return (x + y + a);
}

var fac (var x, var y, var z)
{
    var a = sum (1, 0);
    var b = 0;

    return (a + x - y) * (z + b);
}

var main ()
{
    var cnt = 0;

    var x_z = 40;

    var z = 2;

    while (cnt < 3)
    {
        fac (x_z - z, 1, z);
        cnt = cnt + 1;
    }

    return 0;
}

